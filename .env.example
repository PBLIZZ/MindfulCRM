# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/mindfulcrm

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALLBACK_URL=http://localhost:5000/auth/google/callback

# Alternative OAuth environment variable names (for compatibility)
OAUTH_CLIENT_ID=your_google_client_id_here
OAUTH_CLIENT_SECRET=your_google_client_secret_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_KEY_ENV_VAR=your_openai_api_key_here

# Session Configuration
SESSION_SECRET=your_secure_session_secret_here

# Server Configuration
PORT=5000
NODE_ENV=development

# Replit Configuration (if deploying on Replit)
REPL_ID=your_repl_id_here
REPLIT_DOMAINS=your_replit_domain_here
