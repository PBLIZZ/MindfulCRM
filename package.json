  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build:client": "vite build",
    "build:server": "tsup",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "type-check": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.server.json --noEmit",
    "fix:imports": "node scripts/add-js-extensions.mjs",
    "lint": "NODE_OPTIONS='--max-old-space-size=4096' eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=4096' eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "lint:check": "NODE_OPTIONS='--max-old-space-size=4096' eslint . --ext .js,.jsx,.ts,.tsx --cache --quiet",
    "db:push": "drizzle-kit push",
    "seed": "tsx scripts/seed-test-data.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "NODE_ENV=test jest --testPathPattern=performance",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:llm": "tsx server/tests/test-llm-integration.ts",
    "hooks:test": "pre-commit run --all-files",
    "hooks:install": "pre-commit install && pre-commit install --hook-type pre-push"
  },
